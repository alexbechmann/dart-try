  "version": "0.2.0",
  "configurations": [
    {
      "name": "main",
      "type": "dart",
      "request": "launch",
      "program": "src/main.dart",
      "cwd": "${workspaceFolder}",
      "serverReadyAction": {
        "pattern": "Listening on localhost:([0-9]+)",
        "uriFormat": "http://localhost:%s",
        "action": "openExternally"
      }
    }
  ]
}
